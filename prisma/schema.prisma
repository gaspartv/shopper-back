generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://docker:docker@localhost:5432/shopper-db?schema=public"
}

model Measure {
  measure_uuid     String   @id @default(uuid()) @db.Uuid
  measure_datetime DateTime
  measure_type     String
  has_confirmed    Boolean? @default(false)
  image_url        String
  measure_value    Float
  customer_code    String

  @@map("measures")
}
